<table style="width:728px">
  <thead>
    <tr>
        <th style="text-align:center;">Id</th>
        <th style="text-align:center;">Title</th>
        <th style="text-align:center;">Content</th>
        <th style="text-align:center;">Author</th>
    </tr>
  </thead>
  <tbody>
    <% @articles.each_with_index do |article, index| %>
        <tr>
          <td style="text-align:center;"><%= index+1 %></td>
          <td style="text-align:center;"><%= article.title %></td>
          <td style="text-align:center;"><%= article.content %></td>
          <td style="text-align:center;"><%= link_to article.user.name, user_path(article.user_id) %></td>
        </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @articles, renderer: BootstrapPagination::Rails, class: "pull-left" %>
